function fcn(t)
%#codegen
coder.extrinsic('fprintf', 'datetime');

persistent firstTime

if isempty(firstTime)
    firstTime = false;
    fprintf('Starting Now\n');
end

% Get the current date and time
currentTime = datetime('now', 'Format', 'yyyy-MM-dd HH:mm:ss.SSS');
fprintf('time = %.4f, current time = %s\n', t, char(currentTime));
