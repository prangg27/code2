import carla

def draw_box(world, box, rotation, thickness=0.1, color=carla.Color(), life_time=-1.0, persistent_lines=True):
    # Create a Transform for the box
    transform = carla.Transform(box.location, rotation)
    
    # Draw the box
    world.debug.draw_box(box, transform, thickness=thickness, color=color, life_time=life_time, persistent_lines=persistent_lines)

